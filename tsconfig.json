{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@styles/*": ["./src/styles/*"],
      "@lib/*": ["./src/lib/*"],
      "@pages/*": ["./src/pages/*"],
      "@layouts/*": ["./src/layouts/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
"include": ["src/**/*", "./src/env.d.ts", "**/*.ts", "**/*.tsx"],
"exclude": ["node_modules", "dist"]

}